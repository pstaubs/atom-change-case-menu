{
  "name": "atom-change-case-menu",
  "main": "./lib/change-case-menu",
  "version": "0.0.0",
  "description": "Change case using one of 15 case styles from node-case-change. Features a configurable selection dialogue with live preview, quick style cycle, and menu-based case changing.",
  "keywords": [
  ],
  "activationCommands": {
    "atom-workspace": [
      "change-case-menu:list",
      "change-case-menu:feeling-lucky",
      "change-case-menu:lower",
      "change-case-menu:lowerFirst",
      "change-case-menu:sentence",
      "change-case-menu:title",
      "change-case-menu:upper",
      "change-case-menu:upperFirst",
      "change-case-menu:camel",
      "change-case-menu:constant",
      "change-case-menu:dot",
      "change-case-menu:header",
      "change-case-menu:param",
      "change-case-menu:pascal",
      "change-case-menu:path",
      "change-case-menu:snake",
      "change-case-menu:swap"
    ]
  },
  "repository": "https://github.com/pstaubs/atom-change-case-menu",
  "license": "MIT",
  "author":"Paul G. St-Aubin",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "change-case": "~3.0.0",
    "atom-select-list": "^0.7.0"
  }
}
